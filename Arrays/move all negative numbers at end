class Solution {
  public:
    void segregateElements(vector<int>& arr) {
        // Your code goes here
        int n=arr.size();
        vector<int>neg;
        vector<int>pos,ans;
        for(int i=0;i<n;i++){
            if(arr[i]>=0)
            pos.push_back(arr[i]);
            else neg.push_back(arr[i]);
        }
       for(int x:pos)ans.push_back(x);
       for(int x:neg)ans.push_back(x);
        for(int i=0;i<n;i++)
        arr[i]=ans[i];
    }
};
