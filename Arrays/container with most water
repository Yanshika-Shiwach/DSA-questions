class Solution {
public:
    int maxArea(vector<int>& height) {
      int n=height.size();
      int right=n-1,left=0;
      int water=0;
      while(left<right){
        int h=min(height[left],height[right]);
        int width=right-left;
        water=max(water,h*width);
        if(height[left]<height[right])left++;
        else
         right--;
      }
      return water;
    }
};
