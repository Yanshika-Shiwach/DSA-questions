

class Solution {
  public:
    vector<int> leaders(vector<int>& arr) {
        // code here
        int n=arr.size();
        int maxright=arr[n-1];
        vector<int>ans;
        ans.push_back(maxright);
        for(int i=n-2;i>=0;i--){
            if(arr[i]>=maxright){
                maxright=arr[i];
        
        ans.push_back(maxright);
        }}
        reverse(ans.begin(),ans.end());
        return ans;
       
    }
};
