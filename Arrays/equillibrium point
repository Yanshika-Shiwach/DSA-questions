class Solution {
  public:
    int findEquilibrium(vector<int> &arr) {
        // code here
        int n=arr.size();
        int totalsum=0;
        for(int i=0;i<n;i++){
            totalsum+=arr[i];
        }
        int leftsum=0;
        for(int i=0;i<n;i++){
            int curr=arr[i];
            int rightsum=totalsum-leftsum-curr;
            
            if(leftsum==rightsum){
                return i;
            }
            leftsum+=curr;
        }
        return -1;
    }
};
