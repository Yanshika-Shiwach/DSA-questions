class Solution {
  public:
    int cntSubarrays(vector<int> &nums, int k) {
        // code here
        int n=nums.size();
        int cnt=0;
        int sum=0;
        unordered_map<int,int>mp;
        mp[0]=1;
        for(int i=0;i<n;i++){
          sum+=nums[i];
          if(mp.find(sum-k)!=mp.end()){
              cnt+=mp[sum-k];
          }
          mp[sum]++;
        }
        return cnt;
    }
};
