#include<bits/stdc++.h>
using namespace std;
struct node{
    int data;
    node*left;
    node*right;
    node(int val){
        data=val;
        left=right=NULL;
    }
};
void checkBalanced(node*root,bool&ans){
    if(root==NULL) return;
    int left=0;
    int right=0;
    if(root->left){
        left++;
        checkBalanced(root->left,ans );
       }
       if(root->right){
        right++;
        checkBalanced(root->right,ans);
       }
       if(abs(left-right)>1)
       ans=false;
       return;

}
int main(){
    node*root=new node(1);
    root->left=new node(2);
    root->right=new node(3);
    root->left->left=new node(4);
    root->left->right=new node(5);
    bool ans=true;
    checkBalanced(root,ans);
    if(ans)
    cout<<"balanced tree"<<endl;
    else
    cout<<"not balanced tree"<<endl;
    return 0;
}
